<%= render 'users/form' %>
<script type="text/javascript">
  PagSeguroDirectPayment.setSessionId('<%= @pagseguro_session_id %>');
  var hash = PagSeguroDirectPayment.getSenderHash();
</script>
<button onclick="console.log(PagSeguroDirectPayment.getSenderHash());">TEST</button>
<div class="row">
  <div>
    <h2>Registrar</h2>
  </div>

  <%= form_for(@user) do |f| %>
    <div class="buttonHolder col-sm-1 col-sm-offset-2">
      <%= f.text_field :complete_name, :placeholder =>
          "Nome Completo" %>
      <%= f.text_field :email, :placeholder => "E-mail" %>
      <%= f.password_field :password, :placeholder => "Senha" %>
      <%= f.password_field :password_confirmation,
          :placeholder => "Confirmação de senha" %>
      <%= f.submit "Entrar"%>
    </div>
  <% end %>
</div>
