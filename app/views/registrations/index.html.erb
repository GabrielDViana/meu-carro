<section id="course-content">
  <section id="ruby">
    <section class="detailed-syllabus">
      <div class="wrapper-inside">
        <h3 class="title">Listing registrations</h3>
        <table>
          <thead>
            <tr>
              <th>Course</th>
              <th>Full name</th>
              <th>Company</th>
              <th>Email</th>
              <th>Telephone</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <% @registrations.each do |registration| %>
              <tr>
                <td>
                  <%= registration.course.name %>
                </td>
                <td>
                  <%= registration.full_name %>
                </td>
                <td>
                  <%= registration.company %>
                </td>
                <td>
                  <%= registration.email %>
                </td>
                <td>
                  <%= registration.telephone %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </section>
  </section>
</section>
